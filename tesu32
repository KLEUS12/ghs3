loadstring(game:HttpGet("https://raw.githubusercontent.com/VRXDev2/DestroyRobloxGUI/main/DestroyRobloxGUI.lua"))()

if not game:IsLoaded() then
    game.Loaded:Wait()
end

local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
    Name = "Bananas",
    LoadingTitle = "Bananas Hub загружается...",
    LoadingSubtitle = "by YourName",
    ConfigurationSaving = {
        Enabled = false,
    },
    Discord = {
        Enabled = false,
    },
    KeySystem = false,
    Colors = {
        MainColor = Color3.fromRGB(255, 203, 51),
        HoverColor = Color3.fromRGB(230, 183, 46),
        MinColor = Color3.fromRGB(255, 153, 0),
        CloseColor = Color3.fromRGB(255, 77, 77)
    }
})

wait(6)

local MainTab = Window:CreateTab("Главная", nil)

local MainSection = MainTab:CreateSection("Основные функции")

local WiFiPinchButton = MainTab:CreateButton({
    Name = "Wi-Fi pinch",
    Callback = function()
        Rayfield:Notify({
            Title = "Wi-Fi Pinch",
            Content = "Активация...",
            Duration = 3,
            Image = nil,
            Actions = {
                Ignore = {
                    Name = "Ок",
                    Callback = function()
                    end
                },
            },
        })
        print("Wi-Fi pinch активирован!")
    end,
})

local OtherSection = MainTab:CreateSection("Другое")

Rayfield:LoadConfiguration()
